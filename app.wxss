/* 全局样式文件 */
@import "./styles/Iconfont.wxss";

/* 不支持 通配符 *  */
page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/* 主题颜色变量 原生css也支持 */
page{
  --themeColor:#eb4450;
  /* 定义统一字体大小 */
  /* 假设设计稿的宽度为375px=750rpx 所以14px=28rpx*/
  font-size: 28rpx;
}
image{
  width: 100%;
}